<!DOCTYPE html>
<html>
<head>
  <title>Fitmybitstest2</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="navbar"><%= render 'shared/navbar'%></div>

<% if notice %>
    
    <div id="notice_wrapper">
      <p class="alert alert-success"><span class="glyphicon glyphicon-ok"></span><span class="sr-only">Success:</span><%= notice %>
        <button type="button" class="close" data-dismiss="alert">&times;</button><br>
      </p>
      </div>
    <% end %>
    <% if alert %>
    <div id="notice_wrapper">
      <p class="alert alert-danger text-center"><span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
  <span class="sr-only">Error:</span><%= alert %>
        <button type="button" class="close" data-dismiss="alert">&times;</button><br><br>
              <a class="btn btn-danger btn-xs" href="../intro/introduction">Return to Intro</a>
            </p>
      </p>
      </div>
    <% end %>

<%= yield %>

</body>
</html>
